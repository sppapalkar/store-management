<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>

<table style="..." class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Review</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.email %></td>
        <td><%= feedback.review %></td>

        <td><%= link_to 'Show', feedback %> |
        <%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Home', admin_path %>
